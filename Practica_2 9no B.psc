Proceso Practica_1
	Definir n1, n2, n3, m, r, s Como Real;
	Escribir "Ingresar primer número";
	Leer n1;
	Escribir "Ingresar segundo número";
	Leer n2;
	Escribir "Ingresar tercer número";
	Leer n3;
	m<-n1*n2*n3;
	r<-n1-n2-n3;
	s<-n1+n2+n3;
	Escribir "La multiplicación es ", m;
	Escribir "La resta es ", r;
	Escribir "La suma es ", s;
	
FinProceso
